<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="guilang" nsURI="http://www.sdu.dk/mdsd/guilang/Guilang" nsPrefix="guilang">
  <eClassifiers xsi:type="ecore:EClass" name="GUI">
    <eStructuralFeatures xsi:type="ecore:EReference" name="main" eType="#//Unit" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Unit">
    <eStructuralFeatures xsi:type="ecore:EReference" name="layout" eType="#//Layout"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="specifications" eType="#//Specifications"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Entity">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Layout" eSuperTypes="#//Entity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entities" upperBound="-1"
        eType="#//Entity" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Element" eSuperTypes="#//Entity"/>
  <eClassifiers xsi:type="ecore:EClass" name="Specifications">
    <eStructuralFeatures xsi:type="ecore:EReference" name="specifications" upperBound="-1"
        eType="#//Specification" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Specification">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="#//Element"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="options" upperBound="-1"
        eType="#//Option" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Option"/>
  <eClassifiers xsi:type="ecore:EClass" name="Variable"/>
  <eClassifiers xsi:type="ecore:EClass" name="PresetTextValidate">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Require">
    <eStructuralFeatures xsi:type="ecore:EReference" name="exp" eType="#//Require"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Vertical" eSuperTypes="#//Layout"/>
  <eClassifiers xsi:type="ecore:EClass" name="Horizontal" eSuperTypes="#//Layout"/>
  <eClassifiers xsi:type="ecore:EClass" name="Label" eSuperTypes="#//Element"/>
  <eClassifiers xsi:type="ecore:EClass" name="Button" eSuperTypes="#//Element"/>
  <eClassifiers xsi:type="ecore:EClass" name="Input" eSuperTypes="#//Element"/>
  <eClassifiers xsi:type="ecore:EClass" name="Checkbox" eSuperTypes="#//Element"/>
  <eClassifiers xsi:type="ecore:EClass" name="DimOption" eSuperTypes="#//Option">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="width" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="height" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SizeOption" eSuperTypes="#//Option"/>
  <eClassifiers xsi:type="ecore:EClass" name="TextColor" eSuperTypes="#//Option">
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//ColorVariable"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BackgroundColor" eSuperTypes="#//Option">
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//ColorVariable"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TextValue" eSuperTypes="#//Option"/>
  <eClassifiers xsi:type="ecore:EClass" name="TextValidate" eSuperTypes="#//Option">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//RegexVariable"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="preset" eType="#//PresetTextValidate"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CheckboxValidate" eSuperTypes="#//Option"/>
  <eClassifiers xsi:type="ecore:EClass" name="RequireElements" eSuperTypes="#//Option">
    <eStructuralFeatures xsi:type="ecore:EReference" name="req" eType="#//Require"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ColorVariable" eSuperTypes="#//Variable"/>
  <eClassifiers xsi:type="ecore:EClass" name="RegexVariable" eSuperTypes="#//Variable"/>
  <eClassifiers xsi:type="ecore:EClass" name="Or" eSuperTypes="#//Require">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Require"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Require"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="And" eSuperTypes="#//Require">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Require"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Require"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Not" eSuperTypes="#//Require"/>
  <eClassifiers xsi:type="ecore:EClass" name="ElementRef" eSuperTypes="#//Require">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="#//Element"/>
  </eClassifiers>
</ecore:EPackage>
