regex email_validation = ""
main {
	vertical { 
		label username [text "Username"; dim 2 x 3]
		input emailField
		label passwordField
		input passwordField
		button login
		button register
	}
	elements {
		emailField {
			
			validate email_validation
		}	
		passwordField {
			validate password
		}
		login {
			require emailField && passwordField
		}
		
	}
}