regex email_validation = ""
main {
	vertical {
		label username [text "Username"]
		input emailField
		label passwordField
		input passwordField
		button login
		button register
	}
	elements {
		emailField {
			validate val_email
		}	
		passwordField {
			validate val_password
		}
		login {
			require emailField && passwordField
		}
		
	}
}